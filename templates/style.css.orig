body {
font-family: Verdana,Tahoma,Arial,sans-serif;
background-color: #264C73;
color: #DDEEFF;
text-align: center;
}

.header {
background-color: #264C73;
color: #DDEEFF;
padding: 1px;
margin: 0px;
}

.content {
background-color: #DDEEFF;
color: #111111;
padding: 4px;
border: 3px solid #FFFFFF;
margin: 6px;
}

.footer {
padding: 4px;
}

a:link {
color: #336699;
}

a:visited {
color: #336699;
}

.footer a:link {
color: #8CB3D9;
}

.footer a:visited {
color: #8CB3D9;
}

.header a:link {
color: #8CB3D9;
}

.header a:visited {
color: #8CB3D9;
}


p {
margin: 0.8em 2.0em 0.8em 2.0em;
text-align: left;
}

img {
margin: 1.1em 1.1em 1.1em 1.1em;
text-align: center;
max-width: 94%
}

.subhead {
font-style: italic;
text-align: center;
margin: 0.5em;
}

.copyright {
margin: 1.4em 1.4em 1.4em 1.4em;
font-size: small;
text-align: center;
}

h1 {
font-size: x-large;
font-stretch: wider;
margin: 0px;
border: 0px;
padding: 0px;
}

h1 img {
padding: 0px;
margin: 0px;
}

.banner {
height: 90px;
width: 98%;
}

.centered {
margin-left: auto;
margin-right: auto;
text-align: center;
}
